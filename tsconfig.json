{
    "version": "2.2",
    "compilerOptions": {
      "target": "es5",
      "outDir": "lib",
      "experimentalDecorators": true,
      "sourceMap": true,
      "declaration": true,
      "module": "commonjs",
      "removeComments": false,
      "strictNullChecks": true,
      "noImplicitReturns": true,
      "noImplicitUseStrict": true,
      "noImplicitAny": false,
      "lib": [
        "es5",
        "dom"
      ]
    },
    "exclude": [
        "node_modules",
        "test",
        "lib",
        ".build"
    ]
}




